  "dependencies": {
    "@netology-group/mqtt-client": "0.4.2",
    "@ulms/api-clients": "~0.4.0",
    "sdp-transform": "~2.13.0",
    "wrtc": "netology-group/node-webrtc#feature/enable-h264",
    "yargs": "~15.3.1"
